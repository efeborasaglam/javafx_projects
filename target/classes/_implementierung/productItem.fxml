      </Label>
        <Line endY="35.0" />
        <ImageView fx:id="delete" fitHeight="15.0" fitWidth="107.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/Screenshot%202024-06-08%20113014.png" />
            </image>
            <HBox.margin>
                <Insets left="10.0" right="5.0" top="10.0" />
            </HBox.margin>
        </ImageView>
    </children>
