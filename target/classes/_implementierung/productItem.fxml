         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
        <Line endY="35.0" />
        <ComboBox fx:id="anzahlDropdown">
            <HBox.margin>
                <Insets left="2.0" right="2.0" top="5.0" />
            </HBox.margin>
        </ComboBox>
      <Label fx:id="price11" text="p.">
         <font>
            <Font size="10.0" />
